<Window x:Class="RaisePropertyChangedExample.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <TextBox Text="{Binding Id}" Width="100" Height="25" Margin="5" />
            <TextBox Text="{Binding Name}"  Width="100" Height="25" Margin="5" />
        </StackPanel>
    </Grid>
</Window>
